{
  "name": "ai-productivity-suite",
  "version": "1.0.0",
  "description": "AI-Enhanced Productivity Tools - Smart Task Management, Notes, Focus Timer & Analytics",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "setup": "npm run install-all"
  },
  "keywords": [
    "ai",
    "productivity",
    "task-management",
    "notes",
    "focus-timer",
    "analytics"
  ],
  "author": "AI Productivity Suite",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
